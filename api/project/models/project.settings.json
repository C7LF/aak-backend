{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "slug": {
      "type": "string"
    },
    "fancyGallery": {
      "type": "component",
      "repeatable": false,
      "component": "gallery.fancy-gallery"
    },
    "heroContent": {
      "type": "text"
    }
  }
}
